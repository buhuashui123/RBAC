package com.springboot.service;

import com.springboot.pojo.UserAndRole;

/**
 * 用户角色业务
 */
public interface URService {

    /**
     * 给用户添加角色
     * @param userAndRole 用户角色对象
     * @return 返回值大于0则添加成功，否则添加失败
     */
    int addUR(UserAndRole userAndRole);

    /**
     * 给用户删除角色
     * @param userAndRole 用户角色对象
     * @return 返回值大于0则删除成功，否则删除失败
     */
    int DelUR(UserAndRole userAndRole);
}
